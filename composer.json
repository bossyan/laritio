{
  "name": "zoidboi/laritio",
  "description": "Laravel 6.x package for Publit.io PHP SDK.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "laravel/framework": "^6.0",
    "myclabs/php-enum": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^4.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {},
  "autoload-dev": {
    "psr-4": {
      "ZoidBoi\\Laritio\\": "src",
      "ZoidBoi\\Laritio\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
